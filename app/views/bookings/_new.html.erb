<%= form_for Booking.new do |form| %>
  <%= form.label :firstname %>
  <%= form.text_field :firstname, id:"firstname" %>
  <%= form.label :lastname %>
  <%= form.text_field :lastname, id:"lastname" %>
  <%= form.label :email %>
  <%= form.text_field :email, id:"email" %>
  <%= form.submit "Book now" %>
<% end %>